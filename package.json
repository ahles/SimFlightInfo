    "electron-localshortcut": "^3.2.1",
    "leaflet": "^1.7.1",
    "vue": "^2.5.21",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.4",
    "vue-i18n": "^8.22.0",
    "vuetify": "^2.3.14",
    "vuex": "^3.4.0"
